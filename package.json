    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gapi": "^0.0.3",
    "husky": "^8.0.1",
    "jest-environment-jsdom": "^29.2.2",
