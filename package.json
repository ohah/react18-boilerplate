    "react-router-dom": "^6.4.3",
    "recoil": "^0.7.6",
    "redux": "^4.2.0",
    "styled-components": "^5.3.6",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-eslint": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
